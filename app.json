{
  "expo": {
    "sdkVersion": "52.0.0",
    "scheme": "acme",
    "orientation": "portrait",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": true
          },
          "android": {
            "enableProguardInReleaseBuilds": true,
            "newArchEnabled": true
          }
        }
      ],
      "expo-router",
      "expo-font"
    ],
    "name": "GaitWise",
    "slug": "GaitWise",
    "android": {
      "permissions": [
        "INTERNET"
      ]
    },
    "ios": {
      "supportsTablet": true
    }
  }
}
